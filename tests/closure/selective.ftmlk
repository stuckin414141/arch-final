
let hey_now : int -> int = 
    ftmlk(x : int) {
        let h : int -> int = 
            ftmlk(y : int) {
                let o : int -> int = 
                    ftmlk(z : int) {
                        let mult : int -> int -> int = 
                            ftmlk(a : int, b : int) {
                                a * b
                            }
                        in
                        mult(z, y)
                    }
                in
                o(3)
            }
        in
        h(x)
    };

print(hey_now(9)) //this should print 27, fyi